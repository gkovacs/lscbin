#!/usr/bin/env node
// Generated by LiveScript 1.4.0
(function(){
  var fs, glob, livescript, infiles, i$, len$, filename, outfile, slice$ = [].slice;
  fs = require('fs');
  glob = require('glob');
  livescript = require('livescript');
  infiles = slice$.call(process.argv, 2);
  if (infiles.length === 0) {
    infiles = glob.sync('**').filter(function(x){
      return x.indexOf('node_modules') === -1 && x.indexOf('.git') === -1 && x.slice(-3) === '.ls';
    });
  }
  for (i$ = 0, len$ = infiles.length; i$ < len$; ++i$) {
    filename = infiles[i$];
    if (filename.slice(-3) !== '.ls') {
      continue;
    }
    outfile = filename.slice(0, -3);
